{
    "docsPath": "pages",   
    "webroot": "docs",     
    "versionedDocs": false,
    "latestVersionPath": "",
    "browserSyncConfig": {
        "https": true
    },
    "copyTasks": [
      {
        "name": "script-dependencies",
        "sources": [
          "node_modules/timeago.js/dist/timeago.min.js"
        ],
        "destination": "docs/scripts/dependencies"
      },
      {
        "name": "subpage-scripts",
        "sources": [
          "subpage_scripts/reset_password_form.js"
        ],
        "destination": "docs/scripts/subpage_scripts",
        "watch": true
      },
      {
        "name": "gh-pages-cname",
        "sources": [
          "CNAME"
        ],
        "destination": "docs"
      }
    ]
}
